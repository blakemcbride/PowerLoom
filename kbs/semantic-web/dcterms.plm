(CL:IN-PACKAGE "STELLA")

(IN-MODULE "WEB-CORE")


;;; Forward definitions, so we don't have to import SKOS first which itself references DC Elements:

(DEFRELATION /XMLNS/SKOS/note (?X ?Y)
  :BINARY-RELATION TRUE
  :AXIOMS (/XMLNS/RDFS/label /XMLNS/SKOS/note "note")
  :AXIOMS (/XMLNS/RDFS/isDefinedBy /XMLNS/SKOS/note |http://www.w3.org/2004/02/skos/core|)
  :AXIOMS (/XMLNS/SKOS/definition /XMLNS/SKOS/note "A general note, for any purpose.")
  :AXIOMS (/XMLNS/SKOS/scopeNote /XMLNS/SKOS/note "This property may be used directly, or as a super-property for more specific note types.")
  :AXIOMS (/XMLNS/OWL/AnnotationProperty /XMLNS/SKOS/note))

(DEFRELATION /XMLNS/SKOS/definition (?X ?Y)
  :SUBSET-OF /XMLNS/SKOS/note
  :BINARY-RELATION TRUE
  :AXIOMS (/XMLNS/RDFS/label /XMLNS/SKOS/definition "definition")
  :AXIOMS (/XMLNS/RDFS/isDefinedBy /XMLNS/SKOS/definition |http://www.w3.org/2004/02/skos/core|)
  :AXIOMS (/XMLNS/SKOS/definition /XMLNS/SKOS/definition "A statement or formal explanation of the meaning of a concept.")
  :AXIOMS (/XMLNS/OWL/AnnotationProperty /XMLNS/SKOS/definition))

(DEFRELATION /XMLNS/SKOS/scopeNote (?X ?Y)
  :SUBSET-OF /XMLNS/SKOS/note
  :BINARY-RELATION TRUE
  :AXIOMS (/XMLNS/RDFS/label /XMLNS/SKOS/scopeNote "scope note")
  :AXIOMS (/XMLNS/RDFS/isDefinedBy /XMLNS/SKOS/scopeNote |http://www.w3.org/2004/02/skos/core|)
  :AXIOMS (/XMLNS/SKOS/definition /XMLNS/SKOS/scopeNote "A note that helps to clarify the meaning and/or the use of a concept.")
  :AXIOMS (/XMLNS/OWL/AnnotationProperty /XMLNS/SKOS/scopeNote))


(ASSERT
 (AND (/XMLNS/OWL/Ontology |http://purl.org/NET/dc_owl2dl/terms|) (/XMLNS/RDFS/label |http://purl.org/NET/dc_owl2dl/terms| "DCMI metadata terms")
  (DOCUMENTATION |http://purl.org/NET/dc_owl2dl/terms|
   "OWL 2 DL ontology for Dublin Core metadata terms from the http://purl.org/dc/terms/ namespace; this version is grounded in the DC elements ontology and only uses annotation properties.")
  (/XMLNS/DCT/creator |http://purl.org/NET/dc_owl2dl/terms| |http://simon-reinhardt.de/#me|)
  (/XMLNS/DCT/modified |http://purl.org/NET/dc_owl2dl/terms| "2009-09-07")
  (/XMLNS/DCT/license |http://purl.org/NET/dc_owl2dl/terms| |http://creativecommons.org/licenses/by-sa/3.0/|)
  (/XMLNS/OWL/imports |http://purl.org/NET/dc_owl2dl/terms| |http://purl.org/NET/dc_owl2dl/elements|)
  (/XMLNS/OWL/imports |http://purl.org/NET/dc_owl2dl/terms| |http://purl.org/NET/dc_owl2dl/dcam|)
  (/XMLNS/OWL/imports |http://purl.org/NET/dc_owl2dl/terms| |http://purl.org/NET/dc_owl2dl/dcmitype|)))

(DEFRELATION /XMLNS/DCT/abstract (?X ?Y)
  :SUBSET-OF /XMLNS/DCT/description
  :AXIOMS (/XMLNS/OWL/AnnotationProperty /XMLNS/DCT/abstract)
  :AXIOMS (/XMLNS/RDFS/label /XMLNS/DCT/abstract "Abstract")
  :AXIOMS (/XMLNS/SKOS/definition /XMLNS/DCT/abstract "A summary of the resource."))

(DEFRELATION /XMLNS/DCT/accessRights (?X ?Y)
  :DOCUMENTATION "Access Rights may include information regarding access or restrictions based on privacy, security, or other policies."
  :SUBSET-OF /XMLNS/DCT/rights
  :RANGE /XMLNS/DCT/RightsStatement
  :AXIOMS (/XMLNS/OWL/AnnotationProperty /XMLNS/DCT/accessRights)
  :AXIOMS (/XMLNS/RDFS/label /XMLNS/DCT/accessRights "Access Rights")
  :AXIOMS (/XMLNS/SKOS/definition /XMLNS/DCT/accessRights "Information about who can access the resource or an indication of its security status."))

(DEFRELATION /XMLNS/DCT/accrualMethod (?X ?Y)
  :DOMAIN |http://purl.org/dc/dcmitype/Collection|
  :RANGE /XMLNS/DCT/MethodOfAccrual
  :AXIOMS (/XMLNS/OWL/AnnotationProperty /XMLNS/DCT/accrualMethod)
  :AXIOMS (/XMLNS/RDFS/label /XMLNS/DCT/accrualMethod "Accrual Method")
  :AXIOMS (/XMLNS/SKOS/definition /XMLNS/DCT/accrualMethod "The method by which items are added to a collection."))

(DEFRELATION /XMLNS/DCT/accrualPeriodicity (?X ?Y)
  :DOMAIN |http://purl.org/dc/dcmitype/Collection|
  :RANGE /XMLNS/DCT/Frequency
  :AXIOMS (/XMLNS/OWL/AnnotationProperty /XMLNS/DCT/accrualPeriodicity)
  :AXIOMS (/XMLNS/RDFS/label /XMLNS/DCT/accrualPeriodicity "Accrual Periodicity")
  :AXIOMS (/XMLNS/SKOS/definition /XMLNS/DCT/accrualPeriodicity "The frequency with which items are added to a collection."))

(DEFRELATION /XMLNS/DCT/accrualPolicy (?X ?Y)
  :DOMAIN |http://purl.org/dc/dcmitype/Collection|
  :RANGE /XMLNS/DCT/Policy
  :AXIOMS (/XMLNS/OWL/AnnotationProperty /XMLNS/DCT/accrualPolicy)
  :AXIOMS (/XMLNS/RDFS/label /XMLNS/DCT/accrualPolicy "Accrual Policy")
  :AXIOMS (/XMLNS/SKOS/definition /XMLNS/DCT/accrualPolicy "The policy governing the addition of items to a collection."))

(DEFRELATION /XMLNS/DCT/alternative (?X ?Y)
  :DOCUMENTATION "The distinction between titles and alternative titles is application-specific."
  :SUBSET-OF /XMLNS/DCT/title
  :AXIOMS (/XMLNS/OWL/AnnotationProperty /XMLNS/DCT/alternative)
  :AXIOMS (/XMLNS/RDFS/label /XMLNS/DCT/alternative "Alternative Title")
  :AXIOMS (/XMLNS/SKOS/definition /XMLNS/DCT/alternative "An alternative name for the resource.")
  :AXIOMS (/XMLNS/SKOS/note /XMLNS/DCT/alternative
           "In current practice, this term is used primarily with literal values; however, there are important uses with non-literal values as well. As of December 2007, the DCMI Usage Board is leaving this range unspecified pending an investigation of options."))

(DEFRELATION /XMLNS/DCT/audience (?X ?Y)
  :RANGE /XMLNS/DCT/AgentClass
  :AXIOMS (/XMLNS/OWL/AnnotationProperty /XMLNS/DCT/audience)
  :AXIOMS (/XMLNS/RDFS/label /XMLNS/DCT/audience "Audience")
  :AXIOMS (/XMLNS/SKOS/definition /XMLNS/DCT/audience "A class of entity for whom the resource is intended or useful."))

(DEFRELATION /XMLNS/DCT/available (?X ?Y)
  :SUBSET-OF /XMLNS/DCT/date
  :RANGE LITERAL
  :AXIOMS (/XMLNS/OWL/AnnotationProperty /XMLNS/DCT/available)
  :AXIOMS (/XMLNS/RDFS/label /XMLNS/DCT/available "Date Available")
  :AXIOMS (/XMLNS/SKOS/definition /XMLNS/DCT/available "Date (often a range) that the resource became or will become available."))

(DEFRELATION /XMLNS/DCT/bibliographicCitation (?X ?Y)
  :DOCUMENTATION "Recommended practice is to include sufficient bibliographic detail to identify the resource as unambiguously as possible."
  :SUBSET-OF /XMLNS/DCT/identifier
  :DOMAIN /XMLNS/DCT/BibliographicResource
  :RANGE LITERAL
  :AXIOMS (/XMLNS/OWL/AnnotationProperty /XMLNS/DCT/bibliographicCitation)
  :AXIOMS (/XMLNS/RDFS/label /XMLNS/DCT/bibliographicCitation "Bibliographic Citation")
  :AXIOMS (/XMLNS/SKOS/definition /XMLNS/DCT/bibliographicCitation "A bibliographic reference for the resource."))

(DEFRELATION /XMLNS/DCT/conformsTo (?X ?Y)
  :SUBSET-OF /XMLNS/DCT/relation
  :RANGE /XMLNS/DCT/Standard
  :AXIOMS (/XMLNS/OWL/AnnotationProperty /XMLNS/DCT/conformsTo)
  :AXIOMS (/XMLNS/RDFS/label /XMLNS/DCT/conformsTo "Conforms To")
  :AXIOMS (/XMLNS/SKOS/definition /XMLNS/DCT/conformsTo "An established standard to which the described resource conforms."))

(DEFRELATION /XMLNS/DCT/contributor (?X ?Y)
  :DOCUMENTATION "Examples of a Contributor include a person, an organization, or a service. Typically, the name of a Contributor should be used to indicate the entity."
  :SUBSET-OF /XMLNS/RDFS/contributor
  :RANGE /XMLNS/DCT/Agent
  :AXIOMS (/XMLNS/OWL/AnnotationProperty /XMLNS/DCT/contributor)
  :AXIOMS (/XMLNS/RDFS/label /XMLNS/DCT/contributor "Contributor")
  :AXIOMS (/XMLNS/SKOS/definition /XMLNS/DCT/contributor "An entity responsible for making contributions to the resource."))

(DEFRELATION /XMLNS/DCT/coverage (?X ?Y)
  :DOCUMENTATION "Spatial topic and spatial applicability may be a named place or a location specified by its geographic coordinates. Temporal topic may be a named period, date, or date range. A jurisdiction may be a named administrative entity or a geographic place to which the resource applies. Recommended best practice is to use a controlled vocabulary such as the Thesaurus of Geographic Names [TGN]. Where appropriate, named places or time periods can be used in preference to numeric identifiers such as sets of coordinates or date ranges."
  :SUBSET-OF /XMLNS/RDFS/coverage
  :RANGE /XMLNS/DCT/LocationPeriodOrJurisdiction
  :AXIOMS (/XMLNS/OWL/AnnotationProperty /XMLNS/DCT/coverage)
  :AXIOMS (/XMLNS/RDFS/label /XMLNS/DCT/coverage "Coverage")
  :AXIOMS (/XMLNS/SKOS/definition /XMLNS/DCT/coverage
           "The spatial or temporal topic of the resource, the spatial applicability of the resource, or the jurisdiction under which the resource is relevant.")
  :AXIOMS (/XMLNS/DCT/bibliographicCitation /XMLNS/DCT/coverage "[TGN] http://www.getty.edu/research/tools/vocabulary/tgn/index.html")
  :AXIOMS (/XMLNS/RDFS/seeAlso /XMLNS/DCT/coverage |http://www.getty.edu/research/tools/vocabulary/tgn/index.html|))

(DEFRELATION /XMLNS/DCT/created (?X ?Y)
  :SUBSET-OF /XMLNS/DCT/date
  :RANGE LITERAL
  :AXIOMS (/XMLNS/OWL/AnnotationProperty /XMLNS/DCT/created)
  :AXIOMS (/XMLNS/RDFS/label /XMLNS/DCT/created "Date Created")
  :AXIOMS (/XMLNS/SKOS/definition /XMLNS/DCT/created "Date of creation of the resource."))

(DEFRELATION /XMLNS/DCT/creator (?X ?Y)
  :DOCUMENTATION "Examples of a Creator include a person, an organization, or a service. Typically, the name of a Creator should be used to indicate the entity."
  :SUBSET-OF /XMLNS/RDFS/creator
  :SUBSET-OF /XMLNS/DCT/contributor
  :RANGE /XMLNS/DCT/Agent
  :AXIOMS (/XMLNS/OWL/AnnotationProperty /XMLNS/DCT/creator)
  :AXIOMS (/XMLNS/RDFS/label /XMLNS/DCT/creator "Creator")
  :AXIOMS (/XMLNS/SKOS/definition /XMLNS/DCT/creator "An entity primarily responsible for making the resource."))

(DEFRELATION /XMLNS/DCT/date (?X ?Y)
  :DOCUMENTATION "Date may be used to express temporal information at any level of granularity. Recommended best practice is to use an encoding scheme, such as the W3CDTF profile of ISO 8601 [W3CDTF]."
  :SUBSET-OF /XMLNS/RDFS/date
  :RANGE LITERAL
  :AXIOMS (/XMLNS/OWL/AnnotationProperty /XMLNS/DCT/date)
  :AXIOMS (/XMLNS/RDFS/label /XMLNS/DCT/date "Date")
  :AXIOMS (/XMLNS/SKOS/definition /XMLNS/DCT/date "A point or period of time associated with an event in the lifecycle of the resource.")
  :AXIOMS (/XMLNS/DCT/bibliographicCitation /XMLNS/DCT/date "[W3CDTF] http://www.w3.org/TR/NOTE-datetime")
  :AXIOMS (/XMLNS/RDFS/seeAlso /XMLNS/DCT/date |http://www.w3.org/TR/NOTE-datetime|))

(DEFRELATION /XMLNS/DCT/dateAccepted (?X ?Y)
  :DOCUMENTATION "Examples of resources to which a Date Accepted may be relevant are a thesis (accepted by a university department) or an article (accepted by a journal)."
  :SUBSET-OF /XMLNS/DCT/date
  :RANGE LITERAL
  :AXIOMS (/XMLNS/OWL/AnnotationProperty /XMLNS/DCT/dateAccepted)
  :AXIOMS (/XMLNS/RDFS/label /XMLNS/DCT/dateAccepted "Date Accepted")
  :AXIOMS (/XMLNS/SKOS/definition /XMLNS/DCT/dateAccepted "Date of acceptance of the resource."))

(DEFRELATION /XMLNS/DCT/dateCopyrighted (?X ?Y)
  :SUBSET-OF /XMLNS/DCT/date
  :RANGE LITERAL
  :AXIOMS (/XMLNS/OWL/AnnotationProperty /XMLNS/DCT/dateCopyrighted)
  :AXIOMS (/XMLNS/RDFS/label /XMLNS/DCT/dateCopyrighted "Date Copyrighted")
  :AXIOMS (/XMLNS/SKOS/definition /XMLNS/DCT/dateCopyrighted "Date of copyright."))

(DEFRELATION /XMLNS/DCT/dateSubmitted (?X ?Y)
  :DOCUMENTATION "Examples of resources to which a Date Submitted may be relevant are a thesis (submitted to a university department) or an article (submitted to a journal)."
  :SUBSET-OF /XMLNS/DCT/date
  :RANGE LITERAL
  :AXIOMS (/XMLNS/OWL/AnnotationProperty /XMLNS/DCT/dateSubmitted)
  :AXIOMS (/XMLNS/RDFS/label /XMLNS/DCT/dateSubmitted "Date Submitted")
  :AXIOMS (/XMLNS/SKOS/definition /XMLNS/DCT/dateSubmitted "Date of submission of the resource."))

(DEFRELATION /XMLNS/DCT/description (?X ?Y)
  :DOCUMENTATION "Description may include but is not limited to: an abstract, a table of contents, a graphical representation, or a free-text account of the resource."
  :SUBSET-OF /XMLNS/RDFS/description
  :AXIOMS (/XMLNS/OWL/AnnotationProperty /XMLNS/DCT/description)
  :AXIOMS (/XMLNS/RDFS/label /XMLNS/DCT/description "Description")
  :AXIOMS (/XMLNS/SKOS/definition /XMLNS/DCT/description "An account of the resource."))

(DEFRELATION /XMLNS/DCT/educationLevel (?X ?Y)
  :SUBSET-OF /XMLNS/DCT/audience
  :RANGE /XMLNS/DCT/AgentClass
  :AXIOMS (/XMLNS/OWL/AnnotationProperty /XMLNS/DCT/educationLevel)
  :AXIOMS (/XMLNS/RDFS/label /XMLNS/DCT/educationLevel "Audience Education Level")
  :AXIOMS (/XMLNS/SKOS/definition /XMLNS/DCT/educationLevel
           "A class of entity, defined in terms of progression through an educational or training context, for which the described resource is intended."))

(DEFRELATION /XMLNS/DCT/extent (?X ?Y)
  :SUBSET-OF /XMLNS/DCT/format
  :RANGE /XMLNS/DCT/SizeOrDuration
  :AXIOMS (/XMLNS/OWL/AnnotationProperty /XMLNS/DCT/extent)
  :AXIOMS (/XMLNS/RDFS/label /XMLNS/DCT/extent "Extent")
  :AXIOMS (/XMLNS/SKOS/definition /XMLNS/DCT/extent "The size or duration of the resource."))

(DEFRELATION /XMLNS/DCT/format (?X ?Y)
  :DOCUMENTATION "Examples of dimensions include size and duration. Recommended best practice is to use a controlled vocabulary such as the list of Internet Media Types [MIME]."
  :SUBSET-OF /XMLNS/RDFS/format
  :RANGE /XMLNS/DCT/MediaTypeOrExtent
  :AXIOMS (/XMLNS/OWL/AnnotationProperty /XMLNS/DCT/format)
  :AXIOMS (/XMLNS/RDFS/label /XMLNS/DCT/format "Format")
  :AXIOMS (/XMLNS/SKOS/definition /XMLNS/DCT/format "The file format, physical medium, or dimensions of the resource.")
  :AXIOMS (/XMLNS/DCT/bibliographicCitation /XMLNS/DCT/format "[MIME] http://www.iana.org/assignments/media-types/")
  :AXIOMS (/XMLNS/RDFS/seeAlso /XMLNS/DCT/format |http://www.iana.org/assignments/media-types/|))

(DEFRELATION /XMLNS/DCT/hasFormat (?X ?Y)
  :SUBSET-OF /XMLNS/DCT/relation
  :AXIOMS (/XMLNS/OWL/AnnotationProperty /XMLNS/DCT/hasFormat)
  :AXIOMS (/XMLNS/RDFS/label /XMLNS/DCT/hasFormat "Has Format")
  :AXIOMS (/XMLNS/SKOS/definition /XMLNS/DCT/hasFormat
           "A related resource that is substantially the same as the pre-existing described resource, but in another format.")
  :AXIOMS (/XMLNS/SKOS/note /XMLNS/DCT/hasFormat
           "This term is intended to be used with non-literal values as defined in the DCMI Abstract Model (http://dublincore.org/documents/abstract-model/). As of December 2007, the DCMI Usage Board is seeking a way to express this intention with a formal range declaration."))

(DEFRELATION /XMLNS/DCT/hasPart (?X ?Y)
  :SUBSET-OF /XMLNS/DCT/relation
  :AXIOMS (/XMLNS/OWL/AnnotationProperty /XMLNS/DCT/hasPart)
  :AXIOMS (/XMLNS/RDFS/label /XMLNS/DCT/hasPart "Has Part")
  :AXIOMS (/XMLNS/SKOS/definition /XMLNS/DCT/hasPart "A related resource that is included either physically or logically in the described resource.")
  :AXIOMS (/XMLNS/SKOS/note /XMLNS/DCT/hasPart
